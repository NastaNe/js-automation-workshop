{
  "name": "webdriverio-typescript-project",
  "version": "1.0.0",
  "description": "This project will show you how to start your UI Automation Test with WebdriverIO and TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "npm i &>/dev/null",
    "test": "tsc && ./node_modules/.bin/wdio wdio.conf.js && node ./node_modules/rimraf/bin.js src",
    "generate": "allure generate ../allure-results --clean",
    "report": "allure generate allure-results --clean && allure open",
    "wdio": "wdio"
  },
  "keywords": [
    "webdriverio",
    "typescript"
  ],
  "author": {
    "name": "Nastia Nedosiekova",
    "email": "NastaNe@gmail.com"
  },
  "license": "ISC",
