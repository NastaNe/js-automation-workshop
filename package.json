    "fibers": "^2.0.2",
    "file-exists": "^5.0.1",
    "form-data": "^2.3.2",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.0",
    "mocha-allure-reporter": "^1.3.2",
    "npm": "^5.7.1",
    "process-env": "^1.1.0",
